# Environment Configuration
VITE_APP_NAME=Rumfor Market Tracker
VITE_APP_VERSION=1.0.0
VITE_API_URL=http://localhost:3001/api
VITE_APP_ENV=development

# API Configuration
VITE_API_TIMEOUT=10000
VITE_API_BASE_URL=http://localhost:3001

# Feature Flags
VITE_ENABLE_ANALYTICS=false
VITE_ENABLE_DEBUG_MODE=true

# External Services
VITE_MAP_API_KEY=your_map_api_key_here
VITE_STORAGE_URL=http://localhost:3001/storage

# Authentication
VITE_JWT_SECRET=your_jwt_secret_here
VITE_SESSION_TIMEOUT=86400000

# Email Configuration
VITE_EMAIL_SERVICE_URL=http://localhost:3001/email
VITE_SUPPORT_EMAIL=support@rumfor.com

# File Upload
VITE_MAX_FILE_SIZE=10485760
VITE_ALLOWED_FILE_TYPES=image/jpeg,image/png,image/webp,application/pdf

# Analytics
VITE_GOOGLE_ANALYTICS_ID=your_ga_id_here
VITE_SENTRY_DSN=your_sentry_dsn_here
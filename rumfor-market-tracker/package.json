{
  "name": "rumfor-market-tracker",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "build:analyze": "npm run build && npx vite-bundle-analyzer dist && npx vite-bundle-analyzer dist --reporter json > bundle-analysis.json",
    "build:ci": "npm run type-check && npm run lint && npm run build",
    "lint": "eslint . --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "lint:fix": "eslint . --ext ts,tsx --fix",
    "lint:ci": "eslint . --ext ts,tsx --format json --output-file eslint-report.json",
    "preview": "vite preview",
    "preview:ci": "vite preview --host --port 4173",
    "type-check": "tsc --noEmit",
    "format": "prettier --write \"src/**/*.{ts,tsx,js,jsx,css,md}\"",
    "format:check": "prettier --check \"src/**/*.{ts,tsx,js,jsx,css,md}\"",
    "format:ci": "prettier --check \"src/**/*.{ts,tsx,js,jsx,css,md}\" --format json --output-file prettier-report.json",
    "test": "vitest",
    "test:ui": "vitest --ui",
    "test:run": "vitest run",
    "test:coverage": "vitest run --coverage",
    "test:ci": "vitest run --reporter=json --outputFile=test-results.json",
    "test:e2e": "cd ../e2e-tests && npm run e2e:playwright",
    "test:e2e:ci": "cd ../e2e-tests && E2E_BASE_URL=${E2E_BASE_URL:-http://localhost:4173} npm run e2e:playwright",
    "security:audit": "npm audit --audit-level=moderate",
    "security:audit-ci": "npm audit --audit-level=moderate --json > audit-report.json || true",
    "security:deps": "npx audit-ci --moderate",
    "clean": "rm -rf dist",
    "clean:all": "rm -rf dist node_modules .npm",
    "ci:install": "npm ci --prefer-offline --no-audit",
    "ci:setup": "npm run ci:install && npm run type-check && npm run lint",
    "deploy:pages": "npm run build && mkdir -p ../public && cp -r dist/* ../public/"
  },
  "dependencies": {
    "@hookform/resolvers": "^3.3.2",
    "@radix-ui/react-accordion": "^1.1.2",
    "@radix-ui/react-alert-dialog": "^1.0.5",
    "@radix-ui/react-avatar": "^1.0.4",
    "@radix-ui/react-checkbox": "^1.0.4",
    "@radix-ui/react-dialog": "^1.0.5",
    "@radix-ui/react-dropdown-menu": "^2.0.6",
    "@radix-ui/react-label": "^2.0.2",
    "@radix-ui/react-popover": "^1.0.7",
    "@radix-ui/react-progress": "^1.0.3",
    "@radix-ui/react-radio-group": "^1.1.3",
    "@radix-ui/react-select": "^2.0.0",
    "@radix-ui/react-separator": "^1.0.3",
    "@radix-ui/react-slider": "^1.1.2",
    "@radix-ui/react-switch": "^1.0.3",
    "@radix-ui/react-tabs": "^1.0.4",
    "@radix-ui/react-toast": "^1.1.5",
    "@radix-ui/react-toggle": "^1.0.3",
    "@radix-ui/react-tooltip": "^1.0.7",
    "@sentry/react": "^10.34.0",
    "@sentry/vite-plugin": "^4.6.2",
    "@tanstack/react-query": "^5.8.4",
    "@tanstack/react-query-devtools": "^5.8.4",
    "@types/papaparse": "^5.5.2",
    "axios": "^1.6.0",
    "class-variance-authority": "^0.7.0",
    "clsx": "^2.0.0",
    "date-fns": "^2.30.0",
    "file-saver": "^2.0.5",
    "framer-motion": "^12.33.0",
    "jspdf": "^3.0.4",
    "lucide-react": "^0.292.0",
    "papaparse": "^5.5.3",
    "react": "^18.2.0",
    "react-calendar": "^6.0.0",
    "react-dom": "^18.2.0",
    "react-dropzone": "^14.2.3",
    "react-hook-form": "^7.47.0",
    "react-infinite-scroll-component": "^6.1.0",
    "react-router-dom": "^6.18.0",
    "recharts": "^2.8.0",
    "tailwind-merge": "^2.0.0",
    "vite-bundle-analyzer": "^1.3.2",
    "web-vitals": "^5.1.0",
    "zod": "^3.22.4",
    "zustand": "^4.4.6"
  },
  "devDependencies": {
    "@iconify/json": "^2.2.435",
    "@testing-library/jest-dom": "^6.9.1",
    "@testing-library/react": "^16.3.2",
    "@testing-library/user-event": "^14.6.1",
    "@types/file-saver": "^2.0.7",
    "@types/node": "^20.8.10",
    "@types/react": "^18.2.37",
    "@types/react-dom": "^18.2.15",

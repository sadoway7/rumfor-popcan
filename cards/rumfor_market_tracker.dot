digraph RumforMarketTracker {
    // Root application
    "ROOT" [label="Rumfor Market Tracker\nFarmers Market SaaS Platform\nMobile-First PWA\nReact 18 + Node.js + MongoDB", type="root"];

    // Core Infrastructure (MVP Foundation)
    "ROOT" -> "Infrastructure";
    "ROOT" -> "Authentication";
    "ROOT" -> "Database";
    "ROOT" -> "BackendAPI";

    // MVP Core Features (Sprint 1)
    "ROOT" -> "MarketDiscovery";
    "ROOT" -> "VendorParticipation";
    "ROOT" -> "PromoterManagement";
    "ROOT" -> "AdminDashboard";

    // Community Features (Sprint 2)
    "ROOT" -> "CommunityEngagement";
    "ROOT" -> "SocialFeatures";

    // Advanced Features (Sprint 3+)
    "ROOT" -> "BusinessTools";
    "ROOT" -> "Analytics";
    "ROOT" -> "AdvancedModeration";
    "ROOT" -> "VendorTools";

    // Infrastructure Foundation
    "Infrastructure" [
        label="Infrastructure\nGitLab CI/CD + Docker",
        type="category",
        prompt="Set up GitLab repository, CI/CD pipeline, deployment automation, containerization, and development tooling."
    ];

    "Infrastructure" -> "GitLabSetup";
    "Infrastructure" -> "CIDeployment";
    "Infrastructure" -> "QualityGates";
    "Infrastructure" -> "Containerization";

    "GitLabSetup" [label="GitLab Repository", type="card", prompt="Initialize Git repository, configure GitLab Pages, set up project structure and documentation."];
    "CIDeployment" [label="CI/CD Pipeline", type="card", prompt="Configure automated testing, building, deployment, and quality gates for staging and production."];
    "QualityGates" [label="Code Quality", type="card", prompt="Implement ESLint, TypeScript checks, security scanning with Gitleaks, Playwright E2E testing."];
    "Containerization" [label="Docker & Deployment", type="card", prompt="Set up Docker containers for backend, nginx configuration, production deployment on GitLab Pages."];

    // Authentication System
    "Authentication" [
        label="Authentication System\nJWT + Role-Based Access\n(Visitor/Vendor/Promoter/Admin)",
        type="category",
        prompt="Implement complete user authentication with JWT tokens, role-based access control, email verification, and security features."
    ];

    "Authentication" -> "UserRegistration";
    "Authentication" -> "LoginLogout";
    "Authentication" -> "PasswordReset";
    "Authentication" -> "EmailVerification";
    "Authentication" -> "RoleManagement";
    "Authentication" -> "ProtectedRoutes";

    "UserRegistration" [label="User Registration", type="card", prompt="Build registration form with validation, password strength, terms acceptance, role selection (vendor/promoter)."];
    "LoginLogout" [label="Login/Logout System", type="card", prompt="Create secure login with remember me, session management, automatic logout, role-based dashboards."];
    "PasswordReset" [label="Password Recovery", type="card", prompt="Implement secure password reset with email tokens, expiration, rate limiting, and two-factor authentication."];
    "EmailVerification" [label="Email Verification", type="card", prompt="Build email verification system for account activation with resend, expiration, and verification pages."];
    "RoleManagement" [label="Role-Based Access", type="card", prompt="Implement RBAC for visitor/vendor/promoter/admin roles with ProtectedRoute guards and RoleRoute components."];
    "ProtectedRoutes" [label="Route Protection", type="card", prompt="Create React Router guards for authentication, role-based redirects, and secure navigation."];

    // Database Layer
    "Database" [
        label="Database Models\nMongoDB Atlas",
        type="category",
        prompt="Design and implement MongoDB schemas for users, markets, applications, and all data entities."
    ];

    "Database" -> "UserModel";
    "Database" -> "MarketModel";
    "Database" -> "ApplicationModel";
    "Database" -> "BusinessModels";

    "UserModel" [label="User Schema", type="card", prompt="Design User model with authentication fields, profile data, roles, and relationships."];
    "MarketModel" [label="Market Schema", type="card", prompt="Create Market model for event details, locations, categories, and vendor relationships."];
    "ApplicationModel" [label="Application Schema", type="card", prompt="Build Application model for vendor submissions, status tracking, and review workflow."];
    "BusinessModels" [label="Business Data Models", type="card", prompt="Implement Todo, Expense, Comment, Photo, Notification, and other business domain models."];

    // Backend API Layer
    "BackendAPI" [
        label="Backend API\nExpress + MongoDB",
        type="category",
        prompt="Build complete Express.js API with routes, controllers, middleware, and database integration."
    ];

    "BackendAPI" -> "APIRoutes";
    "BackendAPI" -> "Controllers";
    "BackendAPI" -> "Middleware";
    "BackendAPI" -> "Validation";

    "APIRoutes" [label="API Route Structure", type="card", prompt="Create all route files (auth, markets, applications, users, admin, etc.) with RESTful endpoints."];
    "Controllers" [label="Business Logic", type="card", prompt="Implement controllers for authentication, CRUD operations, business logic, and data processing."];
    "Middleware" [label="API Middleware", type="card", prompt="Build authentication, validation, error handling, CORS, rate limiting, and security middleware."];
    "Validation" [label="Data Validation", type="card", prompt="Implement input validation, sanitization, and error handling for all API endpoints."];

    // Sprint 1: Core Markets Feature
    "MarketDiscovery" [
        label="Market Discovery\nSearch & Browse\n9 Categories + Advanced Filters",
        type="category",
        prompt="Build the core market discovery and search functionality that is the heart of the platform."
    ];

    "MarketDiscovery" -> "MarketSearch";
    "MarketDiscovery" -> "MarketListing";
    "MarketDiscovery" -> "CategoryFilters";
    "MarketDiscovery" -> "AdvancedFilters";
    "MarketDiscovery" -> "MarketTracking";
    "MarketDiscovery" -> "MapIntegration";

    "MarketSearch" [label="Market Search", type="card", prompt="Implement text search with debouncing, result highlighting, and relevance scoring across market names and descriptions."];
    "MarketListing" [label="Market Grid/List View", type="card", prompt="Create responsive MarketGrid component with pagination, sorting, loading states, and card-based layout."];
    "CategoryFilters" [label="Market Categories", type="card", prompt="Build 9 category filters: Farmers Markets, Arts & Crafts, Flea Markets, Food Festivals, Antique Shows, Craft Fairs, Street Fairs, Holiday Markets, Community Events."];
    "AdvancedFilters" [label="Advanced Filtering", type="card", prompt="Implement accessibility, parking, pricing, vendor type filtering with location-based search and real-time results."];
    "MarketTracking" [label="Favorite Markets", type="card", prompt="Allow users to save favorite markets, track upcoming events, and get personalized market recommendations."];
    "MapIntegration" [label="Map & Location Services", type="card", prompt="Integrate Google Maps API with geolocation, map markers, directions, and GPS-based market discovery."];

    // Sprint 1: Vendor Applications
    "VendorParticipation" [
        label="Vendor Applications\nApplication System\nMulti-Step Forms + File Uploads",
        type="category",
        prompt="Create the vendor application system with forms, file uploads, status tracking, and admin review workflow."
    ];

    "VendorParticipation" -> "ApplicationForm";
    "VendorParticipation" -> "ApplicationStatus";
    "VendorParticipation" -> "FileUploads";
    "VendorParticipation" -> "AdminReview";
    "VendorParticipation" -> "ApplicationDetail";
    "VendorParticipation" -> "MyApplications";

    "ApplicationForm" [label="Application Form", type="card", prompt="Build multi-step ApplicationForm component with business info, products, requirements, validation using React Hook Form + Zod."];
    "ApplicationStatus" [label="Status Tracking", type="card", prompt="Create ApplicationStatus component with timeline, notifications, ApplicationActions component, and clear next steps."];
    "FileUploads" [label="Document Upload", type="card", prompt="Implement secure PhotoUploader component for business licenses, vendor photos, and supporting documents with drag-and-drop."];
    "AdminReview" [label="Admin Review", type="card", prompt="Build AdminApplicationsPage and AdminPromoterVerification components for review workflow, approval/rejection."];
    "ApplicationDetail" [label="Application Details", type="card", prompt="Create ApplicationDetailPage with comprehensive view, edit capabilities, and admin controls."];
    "MyApplications" [label="My Applications", type="card", prompt="Build MyApplicationsPage with application list, status overview, and quick actions for vendors."];

    // Sprint 1: Admin Dashboard
    "AdminDashboard" [
        label="Admin Dashboard\nUser & System Management\n7 Admin Pages + Analytics",
        type="category",
        prompt="Build comprehensive admin functionality for user management, system monitoring, content moderation, and platform analytics."
    ];

    "AdminDashboard" -> "UserManagement";
    "AdminDashboard" -> "ApplicationManagement";
    "AdminDashboard" -> "MarketManagement";
    "AdminDashboard" -> "ModerationTools";
    "AdminDashboard" -> "AnalyticsReporting";
    "AdminDashboard" -> "PromoterVerification";
    "AdminDashboard" -> "SupportSystem";

    "UserManagement" [label="User Administration", type="card", prompt="Create AdminUsersPage with UserTable component for search, filtering, role management, account controls."];
    "ApplicationManagement" [label="Application Management", type="card", prompt="Build AdminApplicationsPage with comprehensive application review, approval workflow, bulk operations."];
    "MarketManagement" [label="Market Oversight", type="card", prompt="Create AdminMarketsPage for market monitoring, quality control, and promoter support."];
    "ModerationTools" [label="Content Moderation", type="card", prompt="Build AdminModerationPage with reporting tools, content review, and community standards enforcement."];
    "AnalyticsReporting" [label="Platform Analytics", type="card", prompt="Create AdminAnalyticsPage and AdminAnalyticsDashboard with user metrics, application stats, revenue tracking."];
    "PromoterVerification" [label="Promoter Verification", type="card", prompt="Build AdminPromoterVerification component for promoter onboarding and approval workflow."];
    "SupportSystem" [label="Support Ticket System", type="card", prompt="Create AdminSupportPage for user support tickets, issue resolution, and communication tools."];

    // Sprint 2: Enhanced Markets
    "AdvancedMarkets" [
        label="Advanced Market Features\nDetail Pages & Calendar",
        type="category",
        prompt="Add detailed market pages, calendar integration, advanced filtering, and promoter management tools."
    ];

    "AdvancedMarkets" -> "MarketDetail";
    "AdvancedMarkets" -> "MarketCalendar";
    "AdvancedMarkets" -> "AdvancedFilters";
    "AdvancedMarkets" -> "PromoterTools";

    "MarketDetail" [label="Market Detail Pages", type="card", prompt="Create rich market detail with photos, vendor list, schedule, directions, and community content."];
    "MarketCalendar" [label="Market Calendar", type="card", prompt="Build calendar view showing upcoming markets, recurring events, and seasonal planning."];
    "AdvancedFilters" [label="Advanced Filtering", type="card", prompt="Add accessibility, parking, pricing, vendor type filtering with map integration."];
    "PromoterTools" [label="Promoter Management", type="card", prompt="Create promoter interface for market creation, editing, vendor communication, and analytics."];

    // Sprint 3: Community Features
    "Community" [
        label="Community Features\nSocial Engagement",
        type="category",
        prompt="Build social features for user engagement, content sharing, and community building."
    ];

    "Community" -> "Comments";
    "Community" -> "PhotoSharing";
    "Community" -> "HashtagSystem";
    "Community" -> "UserProfiles";

    "Comments" [label="Comment System", type="card", prompt="Implement threaded comments on markets with moderation and real-time notifications."];
    "PhotoSharing" [label="Photo Gallery", type="card", prompt="Create photo upload, gallery display, and social sharing for market experiences."];
    "HashtagSystem" [label="Hashtags & Trends", type="card", prompt="Build hashtag voting, trending topics, and content discovery system."];
    "UserProfiles" [label="User Profiles", type="card", prompt="Create public profiles with activity history, favorite markets, and community stats."];

    // Sprint 4: Business Features
    "BusinessFeatures" [
        label="Business Features\nPayments & Analytics",
        type="category",
        prompt="Add payment processing, advanced analytics, notifications, and business intelligence."
    ];

    "BusinessFeatures" -> "Notifications";
    "BusinessFeatures" -> "AdvancedAnalytics";
    "BusinessFeatures" -> "PaymentIntegration";
    "BusinessFeatures" -> "BulkOperations";

    "Notifications" [label="Notification System", type="card", prompt="Build email, push, and in-app notifications for applications, markets, and community activity."];
    "AdvancedAnalytics" [label="Business Intelligence", type="card", prompt="Create detailed analytics for promoters, vendors, and platform administrators."];
    "PaymentIntegration" [label="Payment Processing", type="card", prompt="Implement payment processing for application fees, marketplace commissions, and subscriptions."];
    "BulkOperations" [label="Bulk Management", type="card", prompt="Build admin tools for bulk user operations, market management, and system maintenance."];

    // Ongoing Vendor Tools
    "VendorTools" [
        label="Vendor Business Tools\nDashboard + Analytics + Productivity\nTodo + Expenses + Market Calendar",
        type="category",
        prompt="Create comprehensive vendor business tools with dashboard, productivity features, expense tracking, and performance analytics."
    ];

    "VendorTools" -> "VendorDashboard";
    "VendorTools" -> "TodoManagement";
    "VendorTools" -> "ExpenseTracking";
    "VendorTools" -> "MarketCalendar";
    "VendorTools" -> "VendorAnalytics";
    "VendorTools" -> "BusinessPlanning";

    "VendorDashboard" [label="Vendor Portal", type="card", prompt="Build comprehensive VendorAnalyticsDashboard with application status, upcoming markets, trending stats, and quick actions."];
    "TodoManagement" [label="Task Management", type="card", prompt="Create TodoForm, TodoItem, TodoProgress, TodoTemplates components for market preparation and operational checklists."];
    "ExpenseTracking" [label="Expense Management", type="card", prompt="Implement ExpenseForm, ExpenseItem, ExpenseSummary, ExpenseChart components with categories and profit analysis."];
    "MarketCalendar" [label="Market Calendar", type="card", prompt="Build MarketCalendar component showing vendor's assigned markets, schedules, and event preparation timeline."];
    "VendorAnalytics" [label="Vendor Analytics", type="card", prompt="Create vendor-specific analytics showing sales trends, attendance data, and market performance metrics."];
    "BusinessPlanning" [label="Business Planning", type="card", prompt="Build business planning tools for goal setting, market strategy, and long-term vendor development."];
}